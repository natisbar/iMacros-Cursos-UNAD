SET !EXTRACT_TEST_POPUP NO
'SET !TIMEOUT_STEP 0
'SET !ERRORIGNORE YES

TAB T=1
TAG POS={{!LOOP}} TYPE=A ATTR=TXT:*entrega<SP>de<SP>la<SP>actividad* EXTRACT=HREF
SET !VAR1 {{!EXTRACT}}
SET !EXTRACT NULL

'SET !VAR2 {{!LOOP}}

TAB OPEN NEW
'ADD !VAR2 1
'TAB T={{!VAR2}}
TAB T=2
URL GOTO={{!VAR1}}
WAIT SECONDS=1
TAG POS=1 TYPE=A ATTR=TXT:Calificaci*n<SP>avanzada
WAIT SECONDS=0.5
