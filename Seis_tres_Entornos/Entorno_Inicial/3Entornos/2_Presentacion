SET !EXTRACT_TEST_POPUP NO
SET !ERRORIGNORE NO
TAG POS=1 TYPE=DIV ATTR=ID:gridsectionname-1 EXTRACT=TXT
SET !VAR2 EVAL("var aux=\"{{!EXTRACT}}\".replace(/ /g,\"\").toUpperCase(); if(aux == \"INFORMACIÓNINICIAL\") aux=\"Presentaci*n<SP>del<SP>curso<SP>*\"; else if(aux == \"INITIALINFORMATION\") aux=\"*Course<SP>presentation<SP>*\";  else MacroError(\"No se determinó idioma\"); aux;")
SET !EXTRACT NULL
TAG POS=1 TYPE=A ATTR=TXT:{{!VAR2}} EXTRACT=HREF
TAB OPEN NEW
TAB T=2
URL GOTO={{!EXTRACT}}
SET !EXTRACT NULL
TAG XPATH="//div[@id='settingsnav']/ul[1]/li[1]/ul[1]/li[1]/p/a" CONTENT={ENTER}

WAIT SECONDS= 1
TAG POS=1 TYPE=INPUT:CHECKBOX FORM=ID:mform1* ATTR=ID:id_frame CONTENT=YES
TAG POS=1 TYPE=INPUT:CHECKBOX FORM=ID:mform1* ATTR=ID:id_copyright CONTENT=YES
TAG POS=1 TYPE=INPUT:TEXT FORM=ID:mform1* ATTR=ID:id_gradepass CONTENT=0
TAG POS=1 TYPE=INPUT:TEXT FORM=ID:mform1* ATTR=ID:id_maximumgrade CONTENT=0
TAG POS=1 TYPE=SELECT FORM=ID:mform1* ATTR=ID:id_visible CONTENT=%1
TAG POS=1 TYPE=SELECT FORM=ID:mform1* ATTR=ID:id_groupmode CONTENT=%0
SET !ERRORIGNORE YES
TAG POS=1 TYPE=IMG ATTR=ALT:Borrar CONTENT={ENTER}
WAIT SECONDS= 0.5
TAG POS=1 TYPE=BUTTON:BUTTON ATTR=* CONTENT={ENTER}
TAG POS=1 TYPE=BUTTON:BUTTON ATTR=ID:availability_addrestriction_completion CONTENT={ENTER}
WAIT SECONDS= 0.5
TAG POS=1 TYPE=SELECT FORM=ID:mform1* ATTR=NAME:cm CONTENT=$Agenda<SP>del<SP>curso<SP>*
TAG POS=1 TYPE=SELECT FORM=ID:mform1* ATTR=NAME:e CONTENT=%1
TAG POS=1 TYPE=INPUT:SUBMIT ATTR=ID:id_unlockcompletion CONTENT={ENTER}
SET !ERRORIGNORE NO
WAIT SECONDS= 2
TAG POS=1 TYPE=SELECT FORM=ID:mform1* ATTR=ID:id_completion CONTENT=%2
TAG POS=1 TYPE=INPUT:CHECKBOX FORM=ID:mform1* ATTR=ID:id_completionview CONTENT=YES
TAG POS=1 TYPE=INPUT:CHECKBOX FORM=ID:mform1* ATTR=ID:id_completionusegrade CONTENT=NO
TAG POS=1 TYPE=SELECT FORM=ID:mform1* ATTR=ID:id_competency_rule CONTENT=%0
TAG POS=1 TYPE=INPUT:SUBMIT ATTR=ID:id_submitbutton CONTENT={ENTER}
WAIT SECONDS= 4
TAB CLOSE
