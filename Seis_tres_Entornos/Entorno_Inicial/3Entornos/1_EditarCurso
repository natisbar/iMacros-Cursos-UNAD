SET !EXTRACT_TEST_POPUP NO
SET !TIMEOUT_STEP 0
SET !ERRORIGNORE YES

SET !VAR2 Edit<SP>settings
TAB T=1
TAG XPATH="//div[@id='settingsnav']/ul[1]/li[1]/ul[1]/li[1]/p/a" EXTRACT=HREF 
'TAG POS=1 TYPE=A ATTR=TXT:{{!VAR2}} EXTRACT=HREF 
SET !VAR1 {{!EXTRACT}}
SET !EXTRACT NULL

TAB OPEN NEW
TAB T=2
URL GOTO={{!VAR1}}

TAG POS=1 TYPE=SELECT FORM=ID:mform1 ATTR=ID:id_format CONTENT=%grid
WAIT SECONDS=2
TAG POS=1 TYPE=SELECT FORM=ID:mform1 ATTR=ID:id_hiddensections CONTENT=%1
TAG POS=1 TYPE=SELECT FORM=ID:mform1 ATTR=ID:id_coursedisplay CONTENT=%0
TAG POS=1 TYPE=SELECT FORM=ID:mform1 ATTR=ID:id_imagecontainerwidth CONTENT=%210
TAG POS=1 TYPE=SELECT FORM=ID:mform1 ATTR=ID:id_newactivity CONTENT=%1
TAG POS=1 TYPE=SELECT FORM=ID:mform1 ATTR=ID:id_lang CONTENT=%es
TAG POS=1 TYPE=SELECT FORM=ID:mform1 ATTR=ID:id_newsitems CONTENT=%4
TAG POS=1 TYPE=SELECT FORM=ID:mform1 ATTR=ID:id_showgrades CONTENT=%1
TAG POS=1 TYPE=SELECT FORM=ID:mform1 ATTR=ID:id_showreports CONTENT=%1
TAG POS=1 TYPE=SELECT FORM=ID:mform1 ATTR=ID:id_enablecompletion  CONTENT=%1
TAG POS=1 TYPE=SELECT FORM=ID:mform1 ATTR=ID:id_groupmode  CONTENT=%2
TAG POS=1 TYPE=SELECT FORM=ID:mform1 ATTR=ID:id_groupmodeforce  CONTENT=%0
TAG POS=1 TYPE=SELECT FORM=ID:mform1 ATTR=ID:id_defaultgroupingid  CONTENT=%0

TAG POS=1 TYPE=INPUT:SUBMIT FORM=ID:mform1 ATTR=ID:id_saveanddisplay

